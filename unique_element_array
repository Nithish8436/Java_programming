   //another method using the count to find the unique element which element have count=1 then i said to be unique
   private static int finduniqueelement(int arr[])
   {
	  for(int i=0;i<arr.length;i++)
	  {
		  int count=0;
		  for(int j=0;j<arr.length;j++)
		  {
			  if(arr[i]==arr[j])
			  {
				  count++;
			  }
		  }
		  
		  if(count==1)
		  {
			  return arr[i];
		  }
	  }
	  
	  return -1;
   }
   
